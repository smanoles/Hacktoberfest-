#include<iostream>
using namespace std;

int add(int x, int y)
{
  return x + y;
}
int main()
{
  int num1, num2, sum;
  
  cout << "What is your first number : ";
  cin >> num1;
  cout << "What is your second number : "; 
  cin >> num2;
  
  sum = add (num1, num2);
  
  cout << "Your sum is : " << sum; 
  }
  
